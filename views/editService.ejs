<form action="/service/<%= service._id %>" method="POST">
    <label for="location">Service id</label>
    <input type="text" name="service_id" required>
    <label for="location_id">Service:</label>
    <input type="text" name="service" required>

    <label for="outsourced">Outsourced:</label>
  <select name="outsourced" id="outsourced" required>
    <option value="" disabled selected>Select Option</option>
    <option value="Yes">Yes</option>
    <option value="No">No</option>
  </select><br><br>
    <label for="managed">Managed By:</label>
    <input type="text" name="managed_by" required>
    <button type="submit" class="btn btn-danger">Save Changes</button>
</form>
